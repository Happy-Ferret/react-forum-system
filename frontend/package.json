{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/alexvcasillas/react-forum-system.git",
  "author": "Alex Casillas <alexvcasillas@gmail.com>",
  "dependencies": {
    "apollo-boost": "^0.1.18",
    "cratebox": "^0.8.0",
    "cratebox-react": "^1.3.0",
    "draft-js": "npm:draft-js-fork-mxstbr",
    "draft-js-code-editor-plugin": "0.2.1",
    "draft-js-drag-n-drop-plugin": "^2.0.3",
    "draft-js-embed-plugin": "^1.2.0",
    "draft-js-focus-plugin": "^2.2.0",
    "draft-js-image-plugin": "^2.0.6",
    "draft-js-import-markdown": "^1.2.1",
    "draft-js-linkify-plugin": "^2.0.0-beta1",
    "draft-js-markdown-plugin": "^3.0.5",
    "draft-js-plugins-editor": "^2.1.1",
    "draft-js-prism-plugin": "0.1.3",
    "draftjs-to-markdown": "^0.5.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "just-debounce-it": "^1.1.0",
    "next": "^7.0.2",
    "next-with-apollo": "^3.2.1",
    "react": "^16.7.0-alpha.0",
    "react-apollo": "^2.2.4",
    "react-content-loader": "^3.2.0",
    "react-dom": "^16.7.0-alpha.0",
    "snarkdown": "^1.2.2",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
